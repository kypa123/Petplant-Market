create table if not exists plant(
	p_id VARCHAR(10) not null,
	p_name varchar(10),
	p_price integer,
	p_description text,
	p_category varchar(20),
	p_level varchar(20),
	p_period integer,
	primary key (p_id)
);

create table if not exists user(
	usr_name varchar(10) not null,
	usr_id varchar(20),
	usr_pswd varchar(20),
	usr_sex varchar(1),
	primary key (usr_name)
);

insert into user values("길순","gilsoon","gs123","F");
insert into user values("지호","jiho","jiho123","M");
select * from user;